{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-bitwise": null,
      "max-line-length": [true, 80],
      "max-classes-per-file": [true, 1, "exclude-class-expressions"],
      "quotemark": [
        true, "single", "avoid-escape", "avoid-template"
      ]
    },
    "rulesDirectory": []
}
